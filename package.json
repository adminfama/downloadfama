{
    "name": "downloadfama",
    "version": "0.5.4",
    "main": "main.js",
    "description": "Find All My Accounts",
    "author": "Matt Haggard",
    "scripts": {
        "publish": "build --mac --win -p always"
    },
    "devDependencies": {
        "electron": "^1.8.1",
        "electron-builder": "^19.49.3"
    },
    "dependencies": {
        "electron-log": "^1.3.0",
        "electron-updater": "^2.17.7"
    },
    "build": {
        "appId": "com.github.adminfama.electronupdaterexample",
        "mac": {
            "category": "public.app-category.productivity",
            "target": [
                "zip",
                "dmg"
            ]
        },
        "publish": [{
            "provider": "github",
            "owner": "adminfama",
            "repo": "downloadfama",
            "releaseType": "release"
        }]
    }
}